.head_bg_image {
    width: 100%;
    height: 100%;
}

.head_view {
    width: 100%;
    height: 318rpx;
    position: relative;
}

.head_data_view {
    position: absolute;
    left: 0rpx;
    top: 64rpx;
    width: 100%;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding-right: 48rpx;
    box-sizing: border-box;
}

.login_btn {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}

.head_left_view {
    display: flex;
    flex-direction: row;
    align-items: center;
    width: 688rpx;
}

.head_title_view {
    display: flex;
    width: 474rpx;
    margin-right: 20rpx;
    flex-direction: column;
}

.head_image_view {
    margin-left: 48rpx;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 146rpx;
    height: 146rpx;
    border-radius: 100%;
    box-sizing: content-box;
    border: 1px solid #eaeaea;
    background-color: white;
}

.head_image {
    width: 100%;
    height: 100%;
    border-radius: 100%;
}

.sex_image {
    width: 41rpx;
    height: 41rpx;
    position: absolute;
    top: 0rpx;
    right: 0rpx;
}

.user_name {
    font-family: PingFangSC-Medium;
    font-size: 48rpx;
    color: #ffffff;
    letter-spacing: 0;
    text-align: left;
    margin-left: 24rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    box-sizing: border-box;
}

.user_job {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #ffffff;
    letter-spacing: 0;
    line-height: 24rpx;
    text-align: left;
    margin-left: 24rpx;
    margin-top: 24rpx;
}

.my_list_view {
    margin-top: 10rpx;
}

.item_view {
    width: 100%;
    height: 136rpx;
    padding-left: 40rpx;
    box-sizing: border-box;
}

.item_left_view {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.item_title_view {
    width: 100%;
    height: 100%;
    border-bottom: 1rpx solid rgba(225,229,237,0.70);
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding-right: 40rpx;
    box-sizing: border-box;
}

.more_image {
    width: 14rpx;
    height: 24rpx;
}

.item_icon {
    background: #eef1ff;
    width: 56rpx;
    height: 56rpx;
    border-radius: 100%;
}

.item_title_text {
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #2c2b28;
    letter-spacing: 0;
    text-align: left;
    margin-left: 24rpx;
}

.head_more_image {
    width: 14rpx;
    height: 14rpx;
}